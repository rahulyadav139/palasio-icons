{
  "name": "palasio-icons",
  "private": true,
  "version": "0.0.0",
  "main": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run generate && tsc --p ./tsconfig.build.json && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "generate": "find lib -mindepth 1 ! -name 'package.json' -print -delete && svgr assets --icon && npm run merge --path=lib",
    "merge": "node ./scripts/merge-exports.js"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1"
  },
  "devDependencies": {
    "@svgr/cli": "^8.1.0",
    "@types/node": "^20.14.2",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "barrel-exporter": "^1.0.7",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-storybook": "^0.8.0",
    "glob": "^10.4.1",
    "rimraf": "^5.0.7",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-copy": "^0.1.6",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-svgr": "^4.2.0"
  }
}
